						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">Alien invasion</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Wikipedia, the free encyclopedia</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">  (Redirected from <a href="/w/index.php?title=Alien_invader&amp;redirect=no" title="Alien invader">Alien invader</a>)</div>
				<!-- /subtitle -->
