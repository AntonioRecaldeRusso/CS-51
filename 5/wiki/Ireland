<area href="County_Down" shape="poly" coords="303,84,304,84,305,85,305,85,305,87,305,88,305,90,306,92,308,94,309,96,310,97,310,99,311,102,312,102,312,103,312,105,312,109,310,112,309,116,308,116,307,116,306,117,305,115,305,112,305,110,305,109,305,108,306,106,306,106,306,104,306,103,305,100,305,97,304,95,303,94,301,94,301,93,300,93,299,94,297,95,297,96,299,97,299,97,301,98,301,99,301,101,301,102,301,103,301,105,301,106,301,107,302,109,301,111,301,113,301,116,302,117,303,118,304,119,305,120,305,121,305,122,305,123,304,124,303,124,301,125,298,126,296,128,295,126,293,125,291,124,291,126,291,129,289,131,288,132,289,133,289,135,288,136,288,139,286,140,286,141,285,144,283,144,282,144,281,145,281,146,277,145,272,144,268,142,267,142,266,141,264,140,263,139,262,136,262,133,261,131,262,129,261,126,261,123,261,121,262,117,260,116,259,115,258,114,259,112,261,111,262,109,263,107,264,105,266,103,268,103,270,105,272,106,274,104,276,103,277,102,278,102,279,99,280,98,281,97,282,97,283,97,285,97,285,96,286,96,286,94,286,92,286,92,288,91,290,89,291,88,293,87,295,87,297,87,299,86,301,84,303,84,303,84,303,84,303,84" alt="County Down" title="County Down" />
